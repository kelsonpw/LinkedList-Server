{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "name": {
          "$id": "/properties/data/properties/name",
          "type": "string",
          "title": "The Name Schema ",
          "default": "",
          "minLength": 1,
          "maxLength": 55,
          "examples": ["Hooli"]
        },
        "password": {
          "$id": "/properties/data/properties/password",
          "type": "string",
          "title": "The Password Schema ",
          "default": "",
          "minLength": 1,
          "maxLength": 55,
          "examples": ["bloodboy"]
        },
        "email": {
          "$id": "/properties/data/properties/email",
          "type": "string",
          "title": "The Email Schema ",
          "default": "",
          "minLength": 1,
          "maxLength": 55,
          "format": "email",
          "examples": ["gavin@hooli.com"]
        },
        "handle": {
          "$id": "/properties/data/properties/handle",
          "type": "string",
          "title": "The Handle Schema ",
          "default": "",
          "minLength": 1,
          "maxLength": 55,
          "examples": ["@hooli"]
        },
        "logo": {
          "$id": "/properties/data/properties/logo",
          "type": "string",
          "title": "The Logo Schema ",
          "default": "",
          "minLength": 1,
          "maxLength": 120,
          "format": "uri",
          "examples": [
            "https://assets-cdn.github.com/images/modules/logos_page/GitHub-Mark.png"
          ]
        }
      },
      "required": ["name", "password", "email", "handle"]
    }
  },
  "required": ["data"]
}
