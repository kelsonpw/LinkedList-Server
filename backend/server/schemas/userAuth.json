{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "username": {
          "$id": "/properties/data/properties/username",
          "type": "string",
          "title": "The Username Schema ",
          "default": "",
          "examples": ["hueter"]
        },
        "password": {
          "$id": "/properties/data/properties/password",
          "type": "string",
          "title": "The Password Schema ",
          "default": "",
          "examples": ["foo123"]
        }
      },
      "required": ["username", "password"]
    }
  },
  "required": ["data"]
}
